program MultTable2(Input, Output);
uses wincrt;

var I, X : integer;

begin

  for I := 1 to 4 do
      begin
         for X := 1 to 5 do
            write((I * X):6);
         writeln;
         if I = 1 then writeln('     ___________________________');
      end; {For I}

end.

{Nested Multiplication Table 2}
